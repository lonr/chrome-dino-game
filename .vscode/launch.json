  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-chrome",
      "request": "launch",
      "name": "debug dev",
      "url": "http://localhost:8080",
      // "breakOnLoad": true,
      "webRoot": "${workspaceFolder}",
      "sourceMapPathOverrides": {
        "/__parcel_source_root/*": "${webRoot}/*"
      }
    },
    {
      "type": "pwa-chrome",
      "request": "launch",
      "name": "debug dist",
      "url": "http://localhost:8080",
      // "breakOnLoad": true,
      "webRoot": "${workspaceFolder}",
      "sourceMapPathOverrides": {
        "/__parcel_source_root/*": "${webRoot}/*"
      }
    }
  ]
}
